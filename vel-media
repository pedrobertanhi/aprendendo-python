print("Simulado de computador de bordo")
distancia = float(input("Por favor, informe a distancia percorrida"))
tempo = float(input("Por favor, informe o tempo da viagem"))

vel_media = distancia / tempo

print("A velodicade media é de {:.2f} km/h".format(vel_media))
print(f"A velocidade media é de {vel_media:.2f} km/h")
